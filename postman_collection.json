    {
      "name": "Auth",
      "item": [
        {
          "name": "Login (Obtain Token)",
          "request": {
            "method": "POST",
            "header": [{ "key": "Content-Type", "value": "application/json" }],
            "url": "{{baseUrl}}/auth/login/",
            "body": {
              "mode": "raw",
              "raw": "{\n  \"username\": \"admin@example.com\",\n  \"password\": \"ChangeMe123!\"\n}"
            },
            "description": "Returns access/refresh."
          }
        },
        {
          "name": "Refresh Token",
          "request": {
            "method": "POST",
            "header": [{ "key": "Content-Type", "value": "application/json" }],
            "url": "{{baseUrl}}/auth/refresh/",
            "body": {
              "mode": "raw",
              "raw": "{\n  \"refresh\": \"<paste-refresh-token>\"\n}"
            }
          }
        },
        {
          "name": "Me (Current User)",
          "request": {
            "method": "GET",
            "header": [
              { "key": "Authorization", "value": "Bearer {{access_token}}" }
            ],
            "url": "{{baseUrl}}/auth/me/"
