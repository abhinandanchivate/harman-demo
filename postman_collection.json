            "url": "{{baseUrl}}/v1/hl7-parser/ingest/",
            "body": {
              "mode": "raw",
              "raw": "{\n  \"raw_message\": \"MSH|^~\\\\&|SENDING|FAC|RECV|FAC|20230901123045||ADT^A01|123|P|2.5\\nPID|1||12345^^^HOSP^MR||DOE^JOHN||19800101|M\"\n}"
            }
          }
        },
        {
          "name": "HL7 - Parse Status",
          "request": {
            "method": "GET",
            "header": [
              { "key": "Authorization", "value": "Bearer {{access_token}}" }
            ],
            "url": "{{baseUrl}}/v1/hl7-parser/parse-status/{{message_id}}/"
          }
        },
        {
          "name": "HL7 - Batch",
          "request": {
            "method": "POST",
            "header": [
              { "key": "Authorization", "value": "Bearer {{access_token}}" },
              { "key": "Content-Type", "value": "application/json" }
            ],
            "url": "{{baseUrl}}/v1/hl7-parser/batch/",
            "body": {
              "mode": "raw",
              "raw": "{\n  \"messages\": [\n    {\"raw_message\": \"...\"},\n    {\"raw_message\": \"...\"}\n  ]\n}"
            }
          }
        },
        {
          "name": "HL7 - List",
          "request": {
            "method": "GET",
            "header": [
              { "key": "Authorization", "value": "Bearer {{access_token}}" }
            ],
            "url": "{{baseUrl}}/v1/hl7-parser/"
          }
        },
        {
          "name": "HL7 - Retrieve",
          "request": {
            "method": "GET",
            "header": [
              { "key": "Authorization", "value": "Bearer {{access_token}}" }
            ],
            "url": "{{baseUrl}}/v1/hl7-parser/1/"
          }
        }
      ]
